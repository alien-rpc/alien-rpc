{
  "name": "@alien-rpc/client",
  "type": "module",
  "version": "0.0.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./formats/*": "./dist/formats/*.js"
  },
  "dependencies": {
    "ky": "^1.2.2"
  },
  "devDependencies": {
    "@alien-rpc/juri": "workspace:^",
