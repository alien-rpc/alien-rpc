{
  "name": "json-qs",
  "type": "module",
  "version": "0.0.0",
  "exports": {
    "./encode": {
      "types": "./dist/encode.d.ts",
      "default": "./dist/encode.js"
    },
    "./decode": {
      "types": "./dist/decode.d.ts",
      "default": "./dist/decode.js"
    }
  },
  "scripts": {
    "dev": "watchlist src -- pnpm build:dev",
    "build": "pnpm clean && PROD=1 tsup --treeshake=smallest",
    "build:dev": "tsup --sourcemap --no-treeshake",
    "clean": "rimraf dist",
    "lint": "tsc -p . --outDir node_modules/.tmp"
  }
}
